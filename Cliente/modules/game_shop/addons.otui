//lista de addons normais
NormalListPanel < ScrollablePanel
  id: NormalListPanel
  visible: true
  size: 705 395
  anchors.left: parent.left
  anchors.top: parent.top
  margin-left: 32
  margin-top: 100
  vertical-scrollbar: NormalListScrollBar

NormalListScrollBar < VerticalScrollBar
  id: NormalListScrollBar
  visible: true
  anchors.top: NormalListPanel.top
  anchors.bottom: NormalListPanel.bottom
  anchors.left: NormalListPanel.right
  margin-right: 60
  margin-top: -12
  margin-bottom: -10
  opacity: 0.7
  step: 48
  pixels-scroll: true

//lista de addons shinys
