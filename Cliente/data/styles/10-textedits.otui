TextEdit < UITextEdit
  color: #3E3838
  size: 86 20
  text-offset: 5 5
  text-align: left
  padding: 3
  image-source: /images/ui/textedit
  image-border: 1
  selection-color: #111416
  selection-background-color: #999999
  $disabled:
    color: #aaaaaa88

PasswordTextEdit < TextEdit
  text-hidden: true

MultilineTextEdit < TextEdit
  multiline: true
